{
  "name": "jjsware",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "lint": "ng lint --quiet --fix",
    "pw:install": "npx playwright install --with-deps",
    "pw": "npx playwright test",
    "test": "ng test --code-coverage --watch=false",
    "docker:build": "docker build -t jjs98/jjsware-client --platform linux/arm64 .",
    "docker:push": "docker push jjs98/jjsware-client",
    "docker:run": "docker run -p 5000:80 --name jjsware-client jjs98/jjsware-client"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.2",
