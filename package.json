{
  "name": "jjsware",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "lint": "ng lint --quiet --fix",
    "pw:install": "npx playwright install --with-deps",
    "pw": "npx playwright test",
    "test": "ng test --code-coverage --watch=false",
    "docker:build": "node docker-runner.js -action build -tag jjs98/jjsware-client:main",
    "docker:push": "node docker-runner.js -action push -tag jjs98/jjsware-client:main",
    "docker:run": "docker run -d -p 5000:80 --name jjsware-client jjs98/jjsware-client:main"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.5",
    "@angular/cdk": "^16.0.4",
    "@angular/common": "^16.0.5",
